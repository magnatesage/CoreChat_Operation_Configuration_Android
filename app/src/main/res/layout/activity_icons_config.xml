                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_20sdp"
                        android:layout_weight="0.5"
                        android:text="@string/online"
                        android:textColor="@color/darkGray"
                        android:textSize="@dimen/medium_text_size" />

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/spinner_online"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:background="@drawable/bg_spinner"
                        android:dropDownWidth="match_parent"
                        android:entries="@array/dropdown_online_icon_array"
                        android:overlapAnchor="false"
                        android:spinnerMode="dropdown"
                        app:customIconSpinner="@{R.array.dropdown_online_icon_array}" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_10sdp"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <com.example.corechatoperationconfigapp.customviews.CustomTextView
                        android:id="@+id/tv_offline"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_20sdp"
                        android:layout_weight="0.5"
                        android:text="@string/offline"
                        android:textColor="@color/darkGray"
                        android:textSize="@dimen/medium_text_size" />

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/spinner_offline"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:background="@drawable/bg_spinner"
                        android:dropDownWidth="match_parent"
                        android:entries="@array/dropdown_offline_icon_array"
                        android:overlapAnchor="false"
                        android:spinnerMode="dropdown"
                        app:customIconSpinner="@{R.array.dropdown_offline_icon_array}" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_10sdp"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <com.example.corechatoperationconfigapp.customviews.CustomTextView
                        android:id="@+id/tv_upcoming"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_20sdp"
                        android:layout_weight="0.5"
                        android:text="@string/upcoming"
                        android:textColor="@color/darkGray"
                        android:textSize="@dimen/medium_text_size" />

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/spinner_upcoming"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:background="@drawable/bg_spinner"
                        android:dropDownWidth="match_parent"
                        android:entries="@array/dropdown_upcoming_icon_array"
                        android:overlapAnchor="false"
                        android:spinnerMode="dropdown"
                        app:customIconSpinner="@{R.array.dropdown_upcoming_icon_array}" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_10sdp"
                    android:orientation="horizontal"
                    android:weightSum="1">

                    <com.example.corechatoperationconfigapp.customviews.CustomTextView
                        android:id="@+id/tv_pause"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_20sdp"
                        android:layout_weight="0.5"
                        android:text="@string/pause"
                        android:textColor="@color/darkGray"
                        android:textSize="@dimen/medium_text_size" />

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/spinner_pause"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:background="@drawable/bg_spinner"
                        android:dropDownWidth="match_parent"
                        android:entries="@array/dropdown_pause_icon_array"
                        android:overlapAnchor="false"
                        android:spinnerMode="dropdown"
                        app:customIconSpinner="@{R.array.dropdown_pause_icon_array}" />

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_10sdp">

                    <com.example.corechatoperationconfigapp.customviews.CustomButton
                        android:id="@+id/btn_back"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/button_left_corner_round"
                        android:onClick="onClick"
                        android:text="@string/back"
                        android:textAllCaps="false"
                        android:textColor="@color/darkGray"
                        app:customButtonFont="@string/Roboto_Light" />

                    <com.example.corechatoperationconfigapp.customviews.CustomButton
                        android:id="@+id/btn_next"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:background="@drawable/button_right_corner_round"
                        android:onClick="onClick"
                        android:text="@string/next"
                        android:textAllCaps="false"
                        android:textColor="@color/darkGray"
                        app:customButtonFont="@string/Roboto_Light"
                        tools:ignore="RelativeOverlap" />

                </RelativeLayout>

            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>